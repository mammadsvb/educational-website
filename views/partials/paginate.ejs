<div class="d-flex justify-content-center">
        <% if(pagination.totalPages>1){ %>
    <ul class="pagination">
        <% if (pagination.page == 1) { %>
        <li class="page-item disabled"><a class="page-link">First</a></li>
        <% } else { %>
        <li class="page-item"><a class="page-link" href="/<%= baseUrl %>/?page=<%= 1 %>">First</a></li>
        <% } %>
        <% let i = ((pagination.page) > 5 ? (pagination.page) - 4 : 1) %>
        <% if (i !== 1) { %>
        <li class="page-item disabled"><a class="page-link">...</a></li>
        <% } %>
        <% for (; i <= ((pagination.page) + 4) && i <= pagination.totalPages; i++) { %>
        <% if (i == pagination.page) { %>
        <li class="page-item active"><a class="page-link"><%= i %></a></li>
        <% } else { %>
        <li class="page-item"><a class="page-link" href="/<%= baseUrl %>/?page=<%= i %>"><%= i %></a></li>
        <% } %>
        <% if (i == (pagination.page) + 4 && i < pagination.totalPages) { %>
        <li class="page-item disabled"><a class="page-link">...</a></li>
        <% } %>
        <% } %>
        <% if (pagination.page == pagination.totalPages) { %>
        <li class="page-item disabled"><a class="page-link">Last</a></li>
        <% } else { %>
        <li class="page-item"><a class="page-link" href="/<%= baseUrl %>/?page=<%= pagination.totalPages %>">Last</a>
        </li>
        <% } %>
    </ul>
    <% } %>
</div>